{
  "presets": [
    ["es2015", { "modules": false }],
    "stage-2"
  ],
  "plugins": [
    [
      "component",
      {
        "libraryName": "element-ui",
        "styleLibraryName": "theme-chalk"
      }
    ],
    "transform-runtime",
    "lodash"
  ],
  "env": {
    "test": {
      "plugins": [ "istanbul" ]
    }
  }
}